<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <listener>
    <listener-class>com.formation.computerdatabase.initialization.Initialization</listener-class>
  </listener>
  
  <servlet>
  	<servlet-name>ServletAdd</servlet-name>
  	<display-name>ServletAdd</display-name>
  	<description></description>
  	<servlet-class>com.formation.computerdatabase.servlets.computer.ServletAdd</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDashboard</servlet-name>
  	<display-name>ServletDashboard</display-name>
  	<description></description>
  	<servlet-class>com.formation.computerdatabase.servlets.ServletDashboard</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletAdd</servlet-name>
  	<url-pattern>/computer/ajouter/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletDashboard</servlet-name>
  	<url-pattern>/accueil/</url-pattern>
  </servlet-mapping>
  
</web-app>
